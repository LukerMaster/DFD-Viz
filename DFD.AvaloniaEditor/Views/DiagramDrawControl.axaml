<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:DFD.AvaloniaEditor.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DFD.AvaloniaEditor.Views.DiagramDrawControl"
             x:DataType="viewModels:DiagramViewModel">
    <Panel>
        <Polygon Points="0,20 20,20 20,0 0,0" Fill="BlueViolet"></Polygon>
        <ItemsControl ItemsSource="{Binding Provider.VisualGraph.Nodes}">
            <ItemsControl.ItemTemplate>

                <DataTemplate>
                    <Panel>
                        <TextBox Text="{Binding Node.Data.Name}"></TextBox>
                        
                    </Panel>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
	</Panel>

</UserControl>
