<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Add Resources Here -->
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Light">
			<SolidColorBrush x:Key="PrimaryBackground">White</SolidColorBrush>
			<SolidColorBrush x:Key="SecondaryBackground">#BBBBBB</SolidColorBrush>
			<SolidColorBrush x:Key="TertiaryBackground">#666666</SolidColorBrush>
			<SolidColorBrush x:Key="PrimaryText">Black</SolidColorBrush>
			<SolidColorBrush x:Key="SecondaryText">#161616</SolidColorBrush>
			<SolidColorBrush x:Key="PrimaryAccent">#218294</SolidColorBrush>
			<SolidColorBrush x:Key="SecondaryAccent">#c5ebf2</SolidColorBrush>
    </ResourceDictionary>
    <ResourceDictionary x:Key="Dark">
      <SolidColorBrush x:Key="PrimaryBackground">#171717</SolidColorBrush>
      <SolidColorBrush x:Key="SecondaryBackground">#184d57</SolidColorBrush>
			<SolidColorBrush x:Key="TertiaryBackground">#666666</SolidColorBrush>
			<SolidColorBrush x:Key="PrimaryText">#DDDDDD</SolidColorBrush>
      <SolidColorBrush x:Key="SecondaryText">#BBBBBB</SolidColorBrush>
      <SolidColorBrush x:Key="PrimaryAccent">#218294</SolidColorBrush>
      <SolidColorBrush x:Key="SecondaryAccent">#c5ebf2</SolidColorBrush>
    </ResourceDictionary>
  </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
