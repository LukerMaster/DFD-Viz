<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Add Resources Here -->
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Light">
			<SolidColorBrush x:Key="PrimaryBackground">White</SolidColorBrush>
			<SolidColorBrush x:Key="SecondaryBackground">#BBBBBB</SolidColorBrush>
			<SolidColorBrush x:Key="TertiaryBackground">#666666</SolidColorBrush>
			<SolidColorBrush x:Key="PrimaryText">Black</SolidColorBrush>
			<SolidColorBrush x:Key="SecondaryText">#161616</SolidColorBrush>
			<SolidColorBrush x:Key="PrimaryAccent">DarkBlue</SolidColorBrush>
			<SolidColorBrush x:Key="SecondaryAccent">#3232DA</SolidColorBrush>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="PrimaryBackground">#12121A</SolidColorBrush>
            <SolidColorBrush x:Key="SecondaryBackground">#16162A</SolidColorBrush>
			<SolidColorBrush x:Key="TertiaryBackground">#666666</SolidColorBrush>
			<SolidColorBrush x:Key="PrimaryText">#DDDDDD</SolidColorBrush>
            <SolidColorBrush x:Key="SecondaryText">#BBBBBB</SolidColorBrush>
            <SolidColorBrush x:Key="PrimaryAccent">DarkBlue</SolidColorBrush>
            <SolidColorBrush x:Key="SecondaryAccent">#3232DA</SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
